{
  "name": "@swiftbase/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for SwiftBase - a lightweight, type-safe client library",
  "author": "SwiftBase",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swiftbase/swiftbase.git",
    "directory": "packages/typescript-sdk"
  },
  "homepage": "https://github.com/swiftbase/swiftbase/tree/main/packages/typescript-sdk#readme",
  "bugs": {
    "url": "https://github.com/swiftbase/swiftbase/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "clean": "rm -rf node_modules dist coverage docs/api",
    "prepublishOnly": "pnpm run build && pnpm run test:run"
  },
  "keywords": [
    "swiftbase",
    "backend",
    "database",
    "realtime",
    "sdk",
    "typescript",
    "api",
    "client",
    "mongodb",
    "websocket"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "4.0.14",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
